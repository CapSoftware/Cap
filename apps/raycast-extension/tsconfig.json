{
	"compilerOptions": {
		"target": "ES2020",
		"lib": ["ES2020"],
		"module": "CommonJS",
		"moduleResolution": "node",
		"jsx": "react-jsx",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"baseUrl": "."
	},
	"include": ["src/**/*"]
}
