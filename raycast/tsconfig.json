{
  "$schema": "https://www.raycast.com/schemas/tsconfig.json",
  "compilerOptions": {
    "lib": ["ES2023"],
    "module": "ES2022",
    "target": "ES2022",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "include": ["src/**/*", "env.d.ts"]
}
